{
	"name" : "QR-generator",
	"version" : "0.0.1",
	"manifest_version" : 2,
	"description" : "見ているページのURLや右クリックしたURLをQRコードに変換して表示します。",
	"background" : {
		"scripts": ["background.js"]
	},
	"content_scripts" : [
		{
			"matches" : ["*://*/*"],
			"js" : ["content.js", "pattern.js", "qrinfo.js", "QR.js", "Polynomial.js"]
		}
	],
	"permissions" : ["contextMenus"]
}
